<table class="table table-hover">
            <h3>Les Administrateurices</h3>
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Picture</th>
                    <th>Gender</th>
                    <th>City</th>
                    <th>Instrument</th>
                    <th>Wished role</th>
                    <th>Actions</th>
                </tr>
            </thead>
            <tbody>
                {% for admin in admins %}
                <tr>
                    <td>{{ admin.fullName }}</td>
                    <td><img src="{{ asset('uploads/pictures/' ~ admin.picture) }}" class="items-align-right avatar avatar-medium" alt="Avatar de {{ admin.fullName}}"> </td>
                    <td>{{ admin.gender }}</td>
                    <td>{{ admin.city }}</td>
                    <td>instrument</td>
                    <td>{{ admin.wish }}</td>
                    <td> <a href="{{ path ('user_valid', {'slug' : admin.slug, 'wish' : admin.wish})}}" class="btn btn-primary"><i class="fas fa-check"></i></a></td>
                </tr>
                {% endfor %}
            </tbody>
        </table>